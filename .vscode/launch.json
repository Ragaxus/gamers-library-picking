{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "vuejs: chrome",
      "url": "http://localhost:8081",
      //"webRoot": "${workspaceFolder}",
      "webRoot": "C:/Users/ragax/Repos",
      "breakOnLoad": true,
      "pathMapping": {
        "/_karma_webpack_": "${workspaceFolder}",
        "c:": "C:"
      },
      "sourceMapPathOverrides": {
        "webpack:/*": "${webRoot}/*",
        // "/./*": "${webRoot}/*",
        //"/src/*": "${webRoot}/*",
        "/*": "*",
        "/./~/*": "${webRoot}/node_modules/*"
      },
      "preLaunchTask": "vuejs: start"
    }
  ]
}