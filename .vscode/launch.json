{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "name": "vuejs: firefox",
      "reAttach": true,
      "url": "http://localhost:8081",
      "webRoot": "${workspaceFolder}/src",
      "pathMappings": [
        {
          "url": "webpack://gamers-library-picking/src",
          "path": "${workspaceFolder}/src"
        },
        {
          "url": "webpack:///src/",
          "path": "{webRoot}/"
        }
      ]
    }
  ]
}